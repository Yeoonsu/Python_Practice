        "\n",
        "def opencsv(filename):  # 파일을 열 때는 opencsv('test.csv') 라고 써야합니다.\n",
        "  f = open(filename, 'r')\n",
        "  reader = csv.reader(f)\n",
        "  output = []\n",
        "  for i in reader:\n",
        "      output.append(i)\n",
        "  return output\n",
        "  f.close()"
